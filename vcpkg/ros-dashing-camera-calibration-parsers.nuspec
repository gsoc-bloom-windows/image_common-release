<?xml version="1.0"?>
<package xmlns="http://schemas.microsoft.com/packaging/2010/07/nuspec.xsd">
  <metadata>
    <id>ros-dashing-camera-calibration-parsers</id>
    <version>2.1.1</version>
    <title>ros-dashing-camera-calibration-parsers</title>
    <authors>Michael Carroll</authors>
    <owners>OSRF</owners>
    <requireLicenseAcceptance>false</requireLicenseAcceptance>
    <description>camera_calibration_parsers contains routines for reading and writing camera calibration parameters.</description>
    <dependencies>
		<dependency id="ros-dashing-rclcpp"/>
		<dependency id="ros-dashing-sensor-msgs"/>
		<dependency id="ros-dashing-yaml-cpp-vendor"/>

    </dependencies>
  </metadata>
  <files>
    <!-- this section controls what actually gets packaged into the Chocolatey package -->
    <file src="tools\**" target="tools" />
    <!--Building from Linux? You may need this instead: <file src="tools/**" target="tools" />-->
  </files>
</package>
