<?xml version="1.0"?>
<package xmlns="http://schemas.microsoft.com/packaging/2010/07/nuspec.xsd">
  <metadata>
    <id>ros-dashing-camera-info-manager</id>
    <version>2.1.1</version>
    <title>ros-dashing-camera-info-manager</title>
    <authors>Michael Carroll</authors>
    <owners>OSRF</owners>
    <requireLicenseAcceptance>false</requireLicenseAcceptance>
    <description>This package provides a C++ interface for camera calibration information.
 It provides CameraInfo, and handles SetCameraInfo service requests, saving and restoring the camera calibration data.</description>
    <dependencies>
		<dependency id="ros-dashing-camera-calibration-parsers"/>
		<dependency id="ros-dashing-image-transport"/>
		<dependency id="ros-dashing-rclcpp"/>
		<dependency id="ros-dashing-sensor-msgs"/>

    </dependencies>
  </metadata>
  <files>
    <!-- this section controls what actually gets packaged into the Chocolatey package -->
    <file src="tools\**" target="tools" />
    <!--Building from Linux? You may need this instead: <file src="tools/**" target="tools" />-->
  </files>
</package>
